install_symlink {
    name: "etc_dev_symlink",
    recovery: true,
    installed_location: "etc/dev",
    symlink_target: "-r $(PRODUCT_OUT)/recovery/root/dev",
}

prebuilt_etc {
    name: "hdr_samsung_mx.key",
    vendor: true,
    src: ":empty_file",
}

prebuilt_etc {
    name: "null",
    relative_install_path: "dev",
    recovery: true,
    src: ":empty_file",
    required: ["etc_dev_symlink"],
}
