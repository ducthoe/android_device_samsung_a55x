# Android fstab file.
#
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK
#
#<src>                            <mnt_point>          <type>      <mnt_flags and options>                                                                                         <fs_mgr_flags>
system                            /system              erofs       ro                                                                                                              logical,first_stage_mount
system_dlkm                       /system_dlkm         erofs       ro                                                                                                              logical,first_stage_mount
system_ext                        /system_ext          erofs       ro                                                                                                              logical,first_stage_mount
product                           /product             erofs       ro                                                                                                              logical,first_stage_mount
vendor                            /vendor              erofs       ro                                                                                                              logical,first_stage_mount
vendor_dlkm                       /vendor_dlkm         erofs       ro                                                                                                              logical,first_stage_mount
/dev/block/by-name/boot           /boot                emmc        defaults                                                                                                        recoveryonly
/dev/block/by-name/recovery       /recovery            emmc        defaults                                                                                                        recoveryonly
/dev/block/by-name/misc           /misc                emmc        defaults                                                                                                        recoveryonly
/dev/block/by-name/cache          /cache               ext4        noatime,nosuid,nodev,noauto_da_alloc,discard,journal_checksum,data=ordered,errors=panic                         recoveryonly,check
/dev/block/by-name/vbmeta         /vbmeta              emmc        defaults                                                                                                        defaults
/dev/block/by-name/metadata       /metadata            f2fs        noatime,nosuid,nodev,discard,sync,fsync_mode=strict,data_flush                                                  defaults
/dev/block/by-name/userdata       /data                f2fs        noatime,nosuid,nodev,discard,usrquota,grpquota,fsync_mode=nobarrier,reserve_root=32768,resgid=5678              latemount,check,quota,checkpoint=fs,reservedsize=128M,fscompress
/dev/block/by-name/efs            /mnt/vendor/efs      ext4        noatime,nosuid,nodev,noauto_da_alloc,discard,journal_checksum,data=ordered,errors=panic                         defaults
